<Window x:Class="PKS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PKS"
        mc:Ignorable="d"
    Title="Программный комплекс Сотрудника" Height="768" Width="1024" WindowState="Maximized" WindowStartupLocation="CenterScreen" MinWidth="1024" MinHeight="768">
    <Window.Background>
        <ImageBrush ImageSource="/LoginBack.png"/>
    </Window.Background>
    <Grid>
        <Grid Background="#99818181">
            <DataGrid x:Name="Orders" Margin="0" Background="{x:Null}" FontSize="16" IsTextSearchEnabled="True" IsReadOnly="True" MouseDoubleClick="Orders_MouseDoubleClick"/>
        </Grid>
    </Grid>
    </Window>

